<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.geekbrains.chat.client.PrivateMsg">
   <children>
      <TextField fx:id="pMsgField" HBox.hgrow="ALWAYS" />
      <Button fx:id="pMsgSend" mnemonicParsing="false" onAction="#sendPrivateMessage" text="Button" />
   </children>
</HBox>
